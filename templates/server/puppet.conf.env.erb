[<%= @name %>]
<% if @manifest -%>
    manifest       = <%= @manifest %>
<% end -%>
<% if @manifestdir -%>
    manifestdir    = <%= @manifestdir %>
<% end -%>
<% if @modulepath -%>
    modulepath     = <%= [@modulepath].flatten.join(":") %>
<% end -%>
<% if @templatedir -%>
    templatedir    = <%= @templatedir %>
<% end -%>
<% if @config_version -%>
    config_version = <%= @config_version %>
<% end -%>
